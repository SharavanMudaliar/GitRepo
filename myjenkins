pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'in building stage.'
                bat 'javac HelloWorld.java'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deployed') {
            steps {
                echo 'run'
                bat 'java HelloWorld'
            }
        }
    }
}
